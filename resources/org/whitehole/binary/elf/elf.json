    "enumerations": {
        "ObjectFileType": {
            "byteWidth": 2,
            "members": {
                "NONE": {"v": "0x0"},
                "REL": {"v": "0x1"},
                "EXEC": {"v": "0x2"},
                "DYN": {"v": "0x3"},
                "CORE": {"v": "0x4"},
